   ],
   "source": [
    "inputName=input('Enter the order id from purchase records:')\n",
    "qry='delete from purchase where order_id=?'\n",
    "try:\n",
    "        cur.execute(qry, (inputName,))\n",
    "        print ('Order successfully deleted')\n",
    "        conn.commit()\n",
    "except:\n",
    "        print ('Error in deleting order', inputName)\n",
    "        conn.rollback()\n"
   ]
