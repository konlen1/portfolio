    "### Updating NHS number of Nitin Sharma in patient table"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 93,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the new NHS number that you want:5555555555\n",
      "Enter old NHS number:4444444444\n",
      "NHS number is updated.\n"
     ]
    }
   ],
   "source": [
    "inputNewNHSnumber=input('Enter the new NHS number that you want:')\n",
    "inputOldNHSnumber=int(input('Enter old NHS number:'))\n",
    "qry='update patient set NHS=? where NHS=?'\n",
    "\n",
    "try:\n",
    "        cur.execute(qry, (inputNewNHSnumber,inputOldNHSnumber))\n",
    "        print ('NHS number is updated.')\n",
    "        conn.commit()\n",
    "except:\n",
    "        print ('Error in update operation... rollback')\n",
    "        conn.rollback()"
   ]
  },
