   "source": [
    "### Creating Collection and document's structure. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 83,
   "metadata": {},
   "outputs": [],
   "source": [
    "from mongoengine import *\n",
    "class diagnosisReports(Document):\n",
    "    meta= {\"collection\":\"diagnosisReports\"}\n",
    "    _id=StringField(required=True)\n",
    "    NHS=IntField(max_length=10)\n",
    "    complaint=StringField(max_length=500)\n",
    "    testreports=DictField()\n",
    "    xrayimages=ImageField(thumbnail_size=(300,300,False))\n",
    "    treatment=ListField(StringField())\n",
    "    diagnosisreport=StringField(max_length=1000)\n",
    "    prescription=DictField()\n",
    "    staff_id=StringField(max_length=10)\n",
    "    visit_date=DateTimeField"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
